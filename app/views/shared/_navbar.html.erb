
<div class="navbar navbar-expand-sm navbar-light navbar-lewagon sticky-top" style="z-index: 1000;">
  <div class="container-fluid justify-content-between">
    <%= link_to "/#", class: "navbar-brand" do %>
      <%= image_tag "https://assets-global.website-files.com/621aeb805857fc0c3cf0e21d/622f4c2f2ff2cc54d67f5e95_WhatsApp%20Image%202022-03-09%20at%203.18.33%20PM%20copy.jpg", style:  "max-height: 50px;" %>
    <% end %>

    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse justify-content-end" id="navbarSupportedContent">
      <ul class="navbar-nav">
        <li class="nav-item d-lg-flex d-md-flex d-sm-block">
          <% if user_signed_in? %>
            <%= form_for(current_user, url: destroy_user_session_path, method: :delete) do |f| %>
              <%= f.submit "Sign Out", class: "nav-link"  %>
            <% end %>

            <%= link_to "Importacion de CSV", new_archivo_path, class: "nav-link"  %>
          <% end %>
          <%= link_to "Muestreos", muestreos_path, class: "nav-link" %>
          <%= link_to "Informes", archivos_path, class: "nav-link" %>
        </li>
      </ul>
    </div>
  </div>
</div>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    var myNavbar = document.getElementById('navbarSupportedContent');
    var navbarToggler = document.querySelector('.navbar-toggler');

    navbarToggler.addEventListener('click', function() {
      myNavbar.classList.toggle('show');
    });
  });

  document.addEventListener("DOMContentLoaded", function () {
    var myCollapse = new bootstrap.Collapse(document.getElementById('navbarSupportedContent'));
  });


</script>
